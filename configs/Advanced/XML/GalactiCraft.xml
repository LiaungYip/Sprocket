<!-- =================================================================
     Custom Ore Generation "GalactiCraft" Module: This configuration
     covers copper, tin, aluminum, and silicon.
     ================================================================= -->






<!-- Is the "GalactiCraft" mod on the system?  Let's find out! -->
<IfModInstalled name="GalacticraftCore">

    <!-- Starting Configuration for Custom Ore Generation. -->
    <ConfigSection>





        <!-- Setup Screen Configuration -->
        <ConfigSection>
            <OptionDisplayGroup name='groupGalactiCraft' displayName='GalactiCraft' displayState='shown'>
                <Description>
                    Distribution options for GalactiCraft Ores.
                </Description>
            </OptionDisplayGroup>

            <!-- Copper Configuration UI Starting -->
            <ConfigSection>
                <OptionChoice name='glctCopperDist'  displayState='shown' displayGroup='groupGalactiCraft'>
                    <Description> Controls how Copper is generated </Description>
                    <DisplayName>GalactiCraft Copper</DisplayName>
                    <Choice value='LayeredVeins' displayValue='Layered Veins'>
                        <Description>
                            Small, fairly rare motherlodes with 2-4 horizontal veins each.
                        </Description>
                    </Choice>
                    <Choice value='Cloud' displayValue='Strategic Cloud'>
                        <Description>
                            Large irregular clouds filled lightly with ore.
                        </Description>
                    </Choice>
                    <Choice value='Vanilla' displayValue='Vanilla'>
                        <Description>
                            Simulates Vanilla Minecraft.
                        </Description>
                    </Choice>
                    <Choice value='none' displayValue='None' description='Copper is not generated in the world.'/>
                </OptionChoice>
                <OptionNumeric name='glctCopperFreq' default='1'  min='0' max='5' displayState=':= if(?advOptions,"shown","hidden")' displayGroup='groupGalactiCraft'>
                    <Description> Frequency multiplier for GalactiCraft Copper distributions </Description>
                    <DisplayName>GalactiCraft Copper Freq.</DisplayName>
                </OptionNumeric>
                <OptionNumeric name='glctCopperSize' default='1'  min='0' max='5' displayState=':= if(?advOptions,"shown","hidden")' displayGroup='groupGalactiCraft'>
                    <Description> Size multiplier for GalactiCraft Copper distributions </Description>
                    <DisplayName>GalactiCraft Copper Size</DisplayName>
                </OptionNumeric>
            </ConfigSection>
            <!-- Copper Configuration UI Complete -->


            <!-- Tin Configuration UI Starting -->
            <ConfigSection>
                <OptionChoice name='glctTinDist'  displayState='shown' displayGroup='groupGalactiCraft'>
                    <Description> Controls how Tin is generated </Description>
                    <DisplayName>GalactiCraft Tin</DisplayName>
                    <Choice value='LayeredVeins' displayValue='Layered Veins'>
                        <Description>
                            Small, fairly rare motherlodes with 2-4 horizontal veins each.
                        </Description>
                    </Choice>
                    <Choice value='Cloud' displayValue='Strategic Cloud'>
                        <Description>
                            Large irregular clouds filled lightly with ore.
                        </Description>
                    </Choice>
                    <Choice value='Vanilla' displayValue='Vanilla'>
                        <Description>
                            Simulates Vanilla Minecraft.
                        </Description>
                    </Choice>
                    <Choice value='none' displayValue='None' description='Tin is not generated in the world.'/>
                </OptionChoice>
                <OptionNumeric name='glctTinFreq' default='1'  min='0' max='5' displayState=':= if(?advOptions,"shown","hidden")' displayGroup='groupGalactiCraft'>
                    <Description> Frequency multiplier for GalactiCraft Tin distributions </Description>
                    <DisplayName>GalactiCraft Tin Freq.</DisplayName>
                </OptionNumeric>
                <OptionNumeric name='glctTinSize' default='1'  min='0' max='5' displayState=':= if(?advOptions,"shown","hidden")' displayGroup='groupGalactiCraft'>
                    <Description> Size multiplier for GalactiCraft Tin distributions </Description>
                    <DisplayName>GalactiCraft Tin Size</DisplayName>
                </OptionNumeric>
            </ConfigSection>
            <!-- Tin Configuration UI Complete -->


            <!-- Aluminum Configuration UI Starting -->
            <ConfigSection>
                <OptionChoice name='glctAluminumDist'  displayState='shown' displayGroup='groupGalactiCraft'>
                    <Description> Controls how Aluminum is generated </Description>
                    <DisplayName>GalactiCraft Aluminum</DisplayName>
                    <Choice value='LayeredVeins' displayValue='Layered Veins'>
                        <Description>
                            Small, fairly rare motherlodes with 2-4 horizontal veins each.
                        </Description>
                    </Choice>
                    <Choice value='Cloud' displayValue='Strategic Cloud'>
                        <Description>
                            Large irregular clouds filled lightly with ore.
                        </Description>
                    </Choice>
                    <Choice value='Vanilla' displayValue='Vanilla'>
                        <Description>
                            Simulates Vanilla Minecraft.
                        </Description>
                    </Choice>
                    <Choice value='none' displayValue='None' description='Aluminum is not generated in the world.'/>
                </OptionChoice>
                <OptionNumeric name='glctAluminumFreq' default='1'  min='0' max='5' displayState=':= if(?advOptions,"shown","hidden")' displayGroup='groupGalactiCraft'>
                    <Description> Frequency multiplier for GalactiCraft Aluminum distributions </Description>
                    <DisplayName>GalactiCraft Aluminum Freq.</DisplayName>
                </OptionNumeric>
                <OptionNumeric name='glctAluminumSize' default='1'  min='0' max='5' displayState=':= if(?advOptions,"shown","hidden")' displayGroup='groupGalactiCraft'>
                    <Description> Size multiplier for GalactiCraft Aluminum distributions </Description>
                    <DisplayName>GalactiCraft Aluminum Size</DisplayName>
                </OptionNumeric>
            </ConfigSection>
            <!-- Aluminum Configuration UI Complete -->


            <!-- Silicon Configuration UI Starting -->
            <ConfigSection>
                <OptionChoice name='glctSiliconDist'  displayState='shown' displayGroup='groupGalactiCraft'>
                    <Description> Controls how Silicon is generated </Description>
                    <DisplayName>GalactiCraft Silicon</DisplayName>
                    <Choice value='LayeredVeins' displayValue='Layered Veins'>
                        <Description>
                            Small, fairly rare motherlodes with 2-4 horizontal veins each.
                        </Description>
                    </Choice>
                    <Choice value='Cloud' displayValue='Strategic Cloud'>
                        <Description>
                            Large irregular clouds filled lightly with ore.
                        </Description>
                    </Choice>
                    <Choice value='Vanilla' displayValue='Vanilla'>
                        <Description>
                            Simulates Vanilla Minecraft.
                        </Description>
                    </Choice>
                    <Choice value='none' displayValue='None' description='Silicon is not generated in the world.'/>
                </OptionChoice>
                <OptionNumeric name='glctSiliconFreq' default='1'  min='0' max='5' displayState=':= if(?advOptions,"shown","hidden")' displayGroup='groupGalactiCraft'>
                    <Description> Frequency multiplier for GalactiCraft Silicon distributions </Description>
                    <DisplayName>GalactiCraft Silicon Freq.</DisplayName>
                </OptionNumeric>
                <OptionNumeric name='glctSiliconSize' default='1'  min='0' max='5' displayState=':= if(?advOptions,"shown","hidden")' displayGroup='groupGalactiCraft'>
                    <Description> Size multiplier for GalactiCraft Silicon distributions </Description>
                    <DisplayName>GalactiCraft Silicon Size</DisplayName>
                </OptionNumeric>
            </ConfigSection>
            <!-- Silicon Configuration UI Complete -->

        </ConfigSection>
        <!-- Setup Screen Complete -->





        <!-- Overworld Setup Beginning -->

        <IfCondition condition=':= ?COGActive'>

            <!-- Starting Original "Overworld" Block Removal -->

            <Substitute name='glctOverworldBlockSubstitute0' block='minecraft:stone'>
                <Description>
                    Replace vanilla-generated ore clusters.
                </Description>
                <Comment>
                    The global option deferredPopulationRange  must be
                    large enough to catch all ore  clusters (>= 32).
                </Comment>
                <Replaces block='GalacticraftCore:tile.gcBlockCore:5' weight='1.0' />
                <Replaces block='GalacticraftCore:tile.gcBlockCore:6' weight='1.0' />
                <Replaces block='GalacticraftCore:tile.gcBlockCore:7' weight='1.0' />
                <Replaces block='GalacticraftCore:tile.gcBlockCore:8' weight='1.0' />
            </Substitute>

            <!-- Original "Overworld" Block Removal Complete -->

            <!-- Adding blocks -->

            <!-- Begin Copper Generation -->

            <!-- Starting LayeredVeins Preset for Copper. -->
            <ConfigSection>
                <IfCondition condition=':= glctCopperDist = "LayeredVeins"'>
                    <Veins name='glctCopperVeins'  inherits='PresetLayeredVeins' drawWireframe='true' wireframeColor='0x60C17B40' drawBoundBox='false' boundBoxColor='0x60C17B40'>
                        <Description>
                            Small, fairly rare motherlodes with  2-4
                            horizontal veins each.
                        </Description>
                        <OreBlock block='GalacticraftCore:tile.gcBlockCore:5' weight='1.0' />
                        <Replaces block='minecraft:stone' weight='1.0' />
                        <Biome name='.*'  />
                        <Setting name='MotherlodeFrequency' avg=':= 0.1535 * _default_ * glctCopperFreq ' range=':=  _default_ * glctCopperFreq ' type='normal' scaleTo='base' />
                        <Setting name='MotherlodeSize' avg=':= 1.154 * _default_ * glctCopperSize ' range=':=  _default_ * glctCopperSize ' type='normal' />
                        <Setting name='MotherlodeHeight' avg=':= 40 ' range=':=  35 ' type='normal' scaleTo='base' />
                        <Setting name='MotherlodeRangeLimit' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='BranchFrequency' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='BranchInclination' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='BranchLength' avg=':= 1.154 * _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='BranchHeightLimit' avg=':= _default_ ' range=':=  _default_ ' type='normal' scaleTo='base' />
                        <Setting name='SegmentForkFrequency' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='SegmentForkLengthMult' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='SegmentLength' avg=':= _default_ * glctCopperSize ' range=':=  _default_ * glctCopperSize ' type='normal' />
                        <Setting name='SegmentAngle' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='SegmentRadius' avg=':= 1.154 * _default_ * glctCopperSize ' range=':=  _default_ * glctCopperSize ' type='normal' />
                        <Setting name='OreDensity' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='OreRadiusMult' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                    </Veins>
                </IfCondition>
            </ConfigSection>
            <!-- LayeredVeins Preset for Copper is complete. -->


            <!-- Starting Cloud Preset for Copper. -->
            <ConfigSection>
                <IfCondition condition=':= glctCopperDist = "Cloud"'>
                    <Cloud name='glctCopperCloud'  inherits='PresetStrategicCloud' drawWireframe='true' wireframeColor='0x60C17B40' drawBoundBox='false' boundBoxColor='0x60C17B40'>
                        <Description>
                            Large irregular clouds filled lightly
                            with ore.  These are huge, spanning
                            several adjacent chunks, and  consequently
                            rather rare.  They  contain a sizeable
                            amount of ore, but  it takes some time and
                            effort to mine  due to low density. The
                            intent for  strategic clouds is that the
                            player  will need to actively search for
                            one  and then set up a semi-permanent
                            mining base and spend some time  actually
                            mining the ore.
                        </Description>
                        <OreBlock block='GalacticraftCore:tile.gcBlockCore:5' weight='1.0' />
                        <Replaces block='minecraft:stone' weight='1.0' />
                        <Biome name='.*'  />
                        <Setting name='CloudRadius' avg=':= 1.751 * _default_ * glctCopperSize ' range=':=  _default_ * glctCopperSize ' type='normal' />
                        <Setting name='CloudThickness' avg=':= 1.751 * _default_ * glctCopperSize ' range=':=  _default_ * glctCopperSize ' type='normal' scaleTo='base' />
                        <Setting name='DistributionFrequency' avg=':= 3.065  * _default_ * glctCopperFreq ' range=':=  _default_ * glctCopperFreq ' type='normal' scaleTo='base' />
                        <Setting name='CloudHeight' avg=':= 40 ' range=':=  35 ' type='normal' scaleTo='base' />
                        <Setting name='ParentRangeLimit' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='CloudSizeNoise' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='CloudInclination' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='OreDensity' avg=':= _default_ ' range=':=  _default_ ' type='normal' scaleTo='base' />
                        <Setting name='OreVolumeNoiseCutoff' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='OreRadiusMult' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Veins name='glctCopperHintVeins'  inherits='PresetHintVeins' drawWireframe='true' wireframeColor='0x60C17B40' drawBoundBox='false' boundBoxColor='0x60C17B40'>
                            <Description>
                                Single blocks, generously  scattered
                                through all heights  (density is about
                                that of vanilla  iron ore). They will
                                replace dirt  and sandstone (but not
                                grass or  sand), so they can be found
                                nearer to the surface than most  ores.
                                Intened to be used as a  child
                                distribution for large,  rare
                                strategic deposits that  would
                                otherwise be very difficult  to find.
                                Note that the frequency  is multiplied
                                by ground level to  maintain a
                                constant density, but  not by ore
                                frequency because it  is assumed that
                                the frequency of  the parent
                                distribution will  already be scaled
                                by that.
                            </Description>
                            <OreBlock block='GalacticraftCore:tile.gcBlockCore:5' weight='1.0' />
                            <Replaces block='minecraft:dirt' weight='1.0' />
                            <Replaces block='minecraft:sandstone' weight='1.0' />
                        </Veins>
                    </Cloud>
                </IfCondition>
            </ConfigSection>
            <!-- Cloud Preset for Copper is complete. -->


            <!-- Starting Vanilla Preset for Copper. -->
            <ConfigSection>
                <IfCondition condition=':= glctCopperDist = "Vanilla"'>
                    <StandardGen name='glctCopperStandard'  inherits='PresetStandardGen' drawWireframe='true' wireframeColor='0x60C17B40' drawBoundBox='false' boundBoxColor='0x60C17B40'>
                        <Description>
                            A master preset for standardgen ore
                            distributions.
                        </Description>
                        <OreBlock block='GalacticraftCore:tile.gcBlockCore:5' weight='1.0' />
                        <Replaces block='minecraft:stone' weight='1.0' />
                        <Biome name='.*'  />
                        <Setting name='Size' avg=':= 7 * glctCopperSize ' range=':=  _default_ * glctCopperSize ' type='normal' />
                        <Setting name='Frequency' avg=':= 24 * glctCopperFreq ' range=':=  _default_ * glctCopperFreq ' type='normal' scaleTo='base' />
                        <Setting name='Height' avg=':= 40 ' range=':=  35 ' type='normal' scaleTo='base' />
                        <Setting name='ParentRangeLimit' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                    </StandardGen>
                </IfCondition>
            </ConfigSection>
            <!-- Vanilla Preset for Copper is complete. -->

            <!-- End Copper Generation -->


            <!-- Begin Tin Generation -->

            <!-- Starting LayeredVeins Preset for Tin. -->
            <ConfigSection>
                <IfCondition condition=':= glctTinDist = "LayeredVeins"'>
                    <Veins name='glctTinVeins'  inherits='PresetLayeredVeins' drawWireframe='true' wireframeColor='0x60C9C9C9' drawBoundBox='false' boundBoxColor='0x60C9C9C9'>
                        <Description>
                            Small, fairly rare motherlodes with  2-4
                            horizontal veins each.
                        </Description>
                        <OreBlock block='GalacticraftCore:tile.gcBlockCore:6' weight='1.0' />
                        <Replaces block='minecraft:stone' weight='1.0' />
                        <Biome name='.*'  />
                        <Setting name='MotherlodeFrequency' avg=':= 1.470 * _default_ * glctTinFreq ' range=':=  _default_ * glctTinFreq ' type='normal' scaleTo='base' />
                        <Setting name='MotherlodeSize' avg=':= 1.137 * _default_ * glctTinSize ' range=':=  _default_ * glctTinSize ' type='normal' />
                        <Setting name='MotherlodeHeight' avg=':= 33 ' range=':=  27 ' type='normal' scaleTo='base' />
                        <Setting name='MotherlodeRangeLimit' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='BranchFrequency' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='BranchInclination' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='BranchLength' avg=':= 1.137 * _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='BranchHeightLimit' avg=':= _default_ ' range=':=  _default_ ' type='normal' scaleTo='base' />
                        <Setting name='SegmentForkFrequency' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='SegmentForkLengthMult' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='SegmentLength' avg=':= _default_ * glctTinSize ' range=':=  _default_ * glctTinSize ' type='normal' />
                        <Setting name='SegmentAngle' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='SegmentRadius' avg=':= 1.137 * _default_ * glctTinSize ' range=':=  _default_ * glctTinSize ' type='normal' />
                        <Setting name='OreDensity' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='OreRadiusMult' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                    </Veins>
                </IfCondition>
            </ConfigSection>
            <!-- LayeredVeins Preset for Tin is complete. -->


            <!-- Starting Cloud Preset for Tin. -->
            <ConfigSection>
                <IfCondition condition=':= glctTinDist = "Cloud"'>
                    <Cloud name='glctTinCloud'  inherits='PresetStrategicCloud' drawWireframe='true' wireframeColor='0x60C9C9C9' drawBoundBox='false' boundBoxColor='0x60C9C9C9'>
                        <Description>
                            Large irregular clouds filled lightly
                            with ore.  These are huge, spanning
                            several adjacent chunks, and  consequently
                            rather rare.  They  contain a sizeable
                            amount of ore, but  it takes some time and
                            effort to mine  due to low density. The
                            intent for  strategic clouds is that the
                            player  will need to actively search for
                            one  and then set up a semi-permanent
                            mining base and spend some time  actually
                            mining the ore.
                        </Description>
                        <OreBlock block='GalacticraftCore:tile.gcBlockCore:6' weight='1.0' />
                        <Replaces block='minecraft:stone' weight='1.0' />
                        <Biome name='.*'  />
                        <Setting name='CloudRadius' avg=':= 1.713 * _default_ * glctTinSize ' range=':=  _default_ * glctTinSize ' type='normal' />
                        <Setting name='CloudThickness' avg=':= 1.713 * _default_ * glctTinSize ' range=':=  _default_ * glctTinSize ' type='normal' scaleTo='base' />
                        <Setting name='DistributionFrequency' avg=':= 2.935  * _default_ * glctTinFreq ' range=':=  _default_ * glctTinFreq ' type='normal' scaleTo='base' />
                        <Setting name='CloudHeight' avg=':= 33 ' range=':=  27 ' type='normal' scaleTo='base' />
                        <Setting name='ParentRangeLimit' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='CloudSizeNoise' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='CloudInclination' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='OreDensity' avg=':= _default_ ' range=':=  _default_ ' type='normal' scaleTo='base' />
                        <Setting name='OreVolumeNoiseCutoff' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='OreRadiusMult' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Veins name='glctTinHintVeins'  inherits='PresetHintVeins' drawWireframe='true' wireframeColor='0x60C9C9C9' drawBoundBox='false' boundBoxColor='0x60C9C9C9'>
                            <Description>
                                Single blocks, generously  scattered
                                through all heights  (density is about
                                that of vanilla  iron ore). They will
                                replace dirt  and sandstone (but not
                                grass or  sand), so they can be found
                                nearer to the surface than most  ores.
                                Intened to be used as a  child
                                distribution for large,  rare
                                strategic deposits that  would
                                otherwise be very difficult  to find.
                                Note that the frequency  is multiplied
                                by ground level to  maintain a
                                constant density, but  not by ore
                                frequency because it  is assumed that
                                the frequency of  the parent
                                distribution will  already be scaled
                                by that.
                            </Description>
                            <OreBlock block='GalacticraftCore:tile.gcBlockCore:6' weight='1.0' />
                            <Replaces block='minecraft:dirt' weight='1.0' />
                            <Replaces block='minecraft:sandstone' weight='1.0' />
                        </Veins>
                    </Cloud>
                </IfCondition>
            </ConfigSection>
            <!-- Cloud Preset for Tin is complete. -->


            <!-- Starting Vanilla Preset for Tin. -->
            <ConfigSection>
                <IfCondition condition=':= glctTinDist = "Vanilla"'>
                    <StandardGen name='glctTinStandard'  inherits='PresetStandardGen' drawWireframe='true' wireframeColor='0x60C9C9C9' drawBoundBox='false' boundBoxColor='0x60C9C9C9'>
                        <Description>
                            A master preset for standardgen ore
                            distributions.
                        </Description>
                        <OreBlock block='GalacticraftCore:tile.gcBlockCore:6' weight='1.0' />
                        <Replaces block='minecraft:stone' weight='1.0' />
                        <Biome name='.*'  />
                        <Setting name='Size' avg=':= 7 * glctTinSize ' range=':=  _default_ * glctTinSize ' type='normal' />
                        <Setting name='Frequency' avg=':= 22 * glctTinFreq ' range=':=  _default_ * glctTinFreq ' type='normal' scaleTo='base' />
                        <Setting name='Height' avg=':= 33 ' range=':=  27 ' type='normal' scaleTo='base' />
                        <Setting name='ParentRangeLimit' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                    </StandardGen>
                </IfCondition>
            </ConfigSection>
            <!-- Vanilla Preset for Tin is complete. -->

            <!-- End Tin Generation -->


            <!-- Begin Aluminum Generation -->

            <!-- Starting LayeredVeins Preset for Aluminum. -->
            <ConfigSection>
                <IfCondition condition=':= glctAluminumDist = "LayeredVeins"'>
                    <Veins name='glctAluminumVeins'  inherits='PresetLayeredVeins' drawWireframe='true' wireframeColor='0x609CACB8' drawBoundBox='false' boundBoxColor='0x609CACB8'>
                        <Description>
                            Small, fairly rare motherlodes with  2-4
                            horizontal veins each.
                        </Description>
                        <OreBlock block='GalacticraftCore:tile.gcBlockCore:7' weight='1.0' />
                        <Replaces block='minecraft:stone' weight='1.0' />
                        <Biome name='.*'  />
                        <Setting name='MotherlodeFrequency' avg=':= 1.330 * _default_ * glctAluminumFreq ' range=':=  _default_ * glctAluminumFreq ' type='normal' scaleTo='base' />
                        <Setting name='MotherlodeSize' avg=':= 1.100 * _default_ * glctAluminumSize ' range=':=  _default_ * glctAluminumSize ' type='normal' />
                        <Setting name='MotherlodeHeight' avg=':= 25 ' range=':=  20 ' type='normal' scaleTo='base' />
                        <Setting name='MotherlodeRangeLimit' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='BranchFrequency' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='BranchInclination' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='BranchLength' avg=':= 1.100 * _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='BranchHeightLimit' avg=':= _default_ ' range=':=  _default_ ' type='normal' scaleTo='base' />
                        <Setting name='SegmentForkFrequency' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='SegmentForkLengthMult' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='SegmentLength' avg=':= _default_ * glctAluminumSize ' range=':=  _default_ * glctAluminumSize ' type='normal' />
                        <Setting name='SegmentAngle' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='SegmentRadius' avg=':= 1.100 * _default_ * glctAluminumSize ' range=':=  _default_ * glctAluminumSize ' type='normal' />
                        <Setting name='OreDensity' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                        <Setting name='OreRadiusMult' avg=':= _default_ ' range=':=  _default_ ' type='normal' />
                    </Veins>
                </IfCondition>
            </ConfigSection>
            <!-- LayeredVeins Preset for Aluminum is complete. -->


            <!-- Starting Cloud Preset for Aluminum. -->
            <ConfigSection>
                <IfCondition condition=':= glctAluminumDist = "Cloud"'>
                    <Cloud name='glctAluminumCloud'  inherits='PresetStrategicCloud' drawWireframe='true' wireframeColor='0x609CACB8' drawBoundBox='false' boundBoxColor='0x609CACB8'>
                        <Description>
                            Large irregular clouds filled lightly
                            with ore.  These are huge, spanning
                            several adjacent chunks, and  consequently
                            rather rare.  They  contain a sizeable
                            amount of ore, but  it takes some time and
