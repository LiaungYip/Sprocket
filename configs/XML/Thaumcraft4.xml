                    <Veins name='thm4Cinnabar0Veins' block='Thaumcraft:blockCustomOre' inherits='PresetLayeredVeins'>
                        <Description>
                            Small, fairly rare motherlodes with 2-4 horizontal veins each.
                        </Description>
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x609d98f0</WireframeColor>
                        <Setting name='MotherlodeSize' avg=':= 1 * thm4CinnabarSize * _default_' range=':= 1 * thm4CinnabarSize * _default_'/>
                        <Setting name='MotherlodeFrequency' avg=':= 1 * thm4CinnabarFreq * _default_'/>
                        <Setting name='MotherlodeHeight' avg=':= 32/64 * dimension.groundLevel' range=':= 16/64 * dimension.groundLevel' type='normal' scaleTo='Biome'/> 
                        <Setting name='SegmentRadius' avg=':= 1 * thm4CinnabarSize * _default_' range=':= 1 * thm4CinnabarSize * _default_'/>
                        <Replaces block='minecraft:stone'/>
                    </Veins>
                </IfCondition>
                <IfCondition condition=':= thm4CinnabarDist = "strategicCloud"'>
                    <Veins name='thm4Cinnabar0HintVeins' block='Thaumcraft:blockCustomOre' inherits='PresetHintVeins'>
