            <IfCondition condition=':= ?COGActive'>
                <!-- **********   Vanilla Deposit Removal   ************* -->
                <Substitute name='mtlfOverworldOreSubstitute0' block='minecraft:stone'>
                    <Description>
                        Replace vanilla-generated ore clusters.
                    </Description>
                    <Comment>
                        The global option deferredPopulationRange must be large enough to catch all ore clusters (>= 32).
                    </Comment>
                    <Replaces block='Metallurgy:fantasy.ore:0' />
                    <Replaces block='Metallurgy:fantasy.ore:1' />
                    <Replaces block='Metallurgy:fantasy.ore:2' />
                    <Replaces block='Metallurgy:fantasy.ore:4' />
                    <Replaces block='Metallurgy:fantasy.ore:5' />
                    <Replaces block='Metallurgy:fantasy.ore:6' />
                    <Replaces block='Metallurgy:fantasy.ore:7' />
                    <Replaces block='Metallurgy:fantasy.ore:8' />
                    <Replaces block='Metallurgy:fantasy.ore:11' />
                    <Replaces block='Metallurgy:fantasy.ore:13' />
                    <Replaces block='Metallurgy:fantasy.ore:14' />
                </Substitute>
                <!-- Adding ores --> 
                <IfCondition condition=':= mtlfPromethiumDist = "layeredVeins"'>
                    <Veins name='mtlfPromethium0Veins' block='Metallurgy:fantasy.ore' inherits='PresetLayeredVeins'>
                        <Description>
                            Small, fairly rare motherlodes with 2-4 horizontal veins each.
                        </Description>
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x6023E788</WireframeColor>
                        <Setting name='MotherlodeSize' avg=':= 1 * mtlfPromethiumSize * _default_' range=':= 1 * mtlfPromethiumSize * _default_'/>
                        <Setting name='MotherlodeFrequency' avg=':= 1 * mtlfPromethiumFreq * _default_'/>
                        <Setting name='MotherlodeHeight' avg=':= 32/64 * dimension.groundLevel' range=':= 32/64 * dimension.groundLevel' type='normal' scaleTo='Biome'/> 
                        <Setting name='SegmentRadius' avg=':= 1 * mtlfPromethiumSize * _default_' range=':= 1 * mtlfPromethiumSize * _default_'/>
                        <Replaces block='minecraft:stone'/>
                    </Veins>
                </IfCondition>
                <IfCondition condition=':= mtlfPromethiumDist = "vanillaStdGen"'>
                    <StandardGen name='mtlfPromethium0Standard' block='Metallurgy:fantasy.ore' inherits='PresetStandardGen'>
