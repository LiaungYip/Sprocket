                            </Description>
                            <DrawWireframe>:=drawWireframes</DrawWireframe>
                            <WireframeColor>0x60592ab4</WireframeColor>
                            <Setting name='MotherlodeFrequency' avg=':= 2 * 1.2 * 1 * 1 * frstApatiteFreq * _default_' range=':= _default_'/> 
                            <Setting name='MotherlodeHeight' avg=':= 60' range=':= 10' type='normal' scaleTo='Base' /> 
                            <Setting name='MotherlodeRangeLimit' avg=':= 1 * 1 * frstApatiteSize * _default_' range=':= 1 * 1 * frstApatiteSize * _default_'/>
                            <Setting name='OreDensity' avg=':= 1 * 0.75 * _default_' range=':= _default_'/>
                            <BiomeType name='Forest'/>
                        </Veins>
                    </Cloud>
                
                </IfCondition>
                
                <!--  Vanilla distribution of Apatite. -->
                <IfCondition condition=':= frstApatiteDist = "vanillaStdGen"'>
                
                    <StandardGen name='frstApatite0Standard' block='Forestry:resources' inherits='PresetStandardGen'>
                        <Description>
                            This mimics vanilla ore generation.
                        </Description>
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x60592ab4</WireframeColor>
                        <Setting name='Size' avg=':= 1 * 2 * frstApatiteSize * _default_'/>
                        <Setting name='Frequency' avg=':= 1 * 1 * 1 * frstApatiteFreq * _default_'/>
                        <Setting name='Height' avg=':= 60' range=':= 10'/> 
                        <Replaces block='minecraft:stone'/>
                    </StandardGen>
                    
                    <!-- Preferred Biome Distribution Settings -->
                    <StandardGen name='frstApatite1Standard' block='Forestry:resources' inherits='PresetStandardGen'>
                        <Description>
                            This mimics vanilla ore generation.
                            2 times as likely in preferred biomes.
                        </Description>
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x60592ab4</WireframeColor>
                        <Setting name='Size' avg=':= 1 * 2 * frstApatiteSize * _default_'/>
                        <Setting name='Frequency' avg=':= 2 * 1 * 1 * frstApatiteFreq * _default_'/>
                        <Setting name='Height' avg=':= 60' range=':= 10'/> 
                        <Replaces block='minecraft:stone'/>
                        <BiomeType name='Forest'/>
                    </StandardGen>
                
                </IfCondition>
                
                <!-- Copper Generation. --> 
                
                <!-- LayeredVeins distribution of Copper. -->
                <IfCondition condition=':= frstCopperDist = "layeredVeins"'>
                
                    <Veins name='frstCopper0Veins' block='Forestry:resources:1' inherits='PresetLayeredVeins'>
                        <Description>
                            Small, fairly rare motherlodes with 2-4 horizontal veins each.
                        </Description>
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x60592ab4</WireframeColor>
                        <Setting name='MotherlodeSize' avg=':= 1 * 1 * frstCopperSize * _default_' range=':= 1 * 1 * frstCopperSize * _default_'/>
                        <Setting name='MotherlodeFrequency' avg=':= 1 * 1 * 0.9 * frstCopperFreq * _default_'/>
                        <Setting name='MotherlodeHeight' avg=':= 43' range=':= 10.5' type='normal' scaleTo='Base'/> 
                        <Setting name='SegmentRadius' avg=':= 1 * 1 * frstCopperSize * _default_' range=':= 1 * 1 * frstCopperSize * _default_'/>
                        <Setting name='OreDensity' avg=':= 1 * 1 * _default_' range=':= _default_'/>
