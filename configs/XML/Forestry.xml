
<!-- ================================================================ 

Custom Ore Generation:   Forestry Module

Generates: Apatite, Copper, Tin

================================================================ -->

    <!-- Mod detection -->
    <IfModInstalled name="Forestry">
        
        <!-- Starting Custom Ore Gen Configuration. -->
        <ConfigSection>
        
            
            <!-- Setup Screen Configuration -->
            <ConfigSection>
                <OptionDisplayGroup name='groupForestry' displayName='Forestry' displayState='shown'> 
                    <Description>
                        Distribution options for Forestry Ores.
                    </Description>
                </OptionDisplayGroup>
                
                <!-- Apatite Configuration UI Starting -->
                <ConfigSection>
                    <OptionChoice name='frstApatiteDist'  displayState='shown' displayGroup='groupForestry'> 
                        <Description> Controls how Apatite is generated </Description> 
                        <DisplayName>Forestry Apatite</DisplayName>
                        <Choice value='sparseVeins' displayValue='Sparse Veins'>
                            <Description>
                                Sparse Veins.
                            </Description>
                        </Choice>
                        <Choice value='strategicCloud' displayValue='Clouds'>
                            <Description>
                                Strategic Clouds.
                            </Description>
                        </Choice>
                        <Choice value='vanillaStdGen' displayValue='Vanilla'>
                            <Description>
                                Vanilla-style clusters.
                            </Description>
                        </Choice>
                        <Choice value='none' displayValue='None' description='Apatite is not generated in the world.'/>
                    </OptionChoice>
                    <OptionNumeric name='frstApatiteFreq' default='1'  min='0' max='5' displayState=':= if(?advOptions,"shown","hidden")' displayGroup='groupForestry'>
                        <Description> Frequency multiplier for Forestry Apatite distributions </Description>
                        <DisplayName>Forestry Apatite Freq.</DisplayName>
                    </OptionNumeric>
                    <OptionNumeric name='frstApatiteSize' default='1'  min='0' max='5' displayState=':= if(?advOptions,"shown","hidden")' displayGroup='groupForestry'>
                        <Description> Size multiplier for Forestry Apatite distributions </Description>
                        <DisplayName>Forestry Apatite Size</DisplayName>
                    </OptionNumeric>
                </ConfigSection> 
                <!-- Apatite Configuration UI Complete -->
                
                
                <!-- Copper Configuration UI Starting -->
                <ConfigSection>
                    <OptionChoice name='frstCopperDist'  displayState='shown' displayGroup='groupForestry'> 
                        <Description> Controls how Copper is generated </Description> 
                        <DisplayName>Forestry Copper</DisplayName>
                        <Choice value='layeredVeins' displayValue='Layered Veins'>
                            <Description>
                                Layered Veins.
                            </Description>
                        </Choice>
                        <Choice value='strategicCloud' displayValue='Clouds'>
                            <Description>
                                Strategic Clouds.
                            </Description>
                        </Choice>
                        <Choice value='vanillaStdGen' displayValue='Vanilla'>
                            <Description>
                                Vanilla-style clusters.
                            </Description>
                        </Choice>
                        <Choice value='none' displayValue='None' description='Copper is not generated in the world.'/>
                    </OptionChoice>
                    <OptionNumeric name='frstCopperFreq' default='1'  min='0' max='5' displayState=':= if(?advOptions,"shown","hidden")' displayGroup='groupForestry'>
                        <Description> Frequency multiplier for Forestry Copper distributions </Description>
                        <DisplayName>Forestry Copper Freq.</DisplayName>
                    </OptionNumeric>
                    <OptionNumeric name='frstCopperSize' default='1'  min='0' max='5' displayState=':= if(?advOptions,"shown","hidden")' displayGroup='groupForestry'>
                        <Description> Size multiplier for Forestry Copper distributions </Description>
                        <DisplayName>Forestry Copper Size</DisplayName>
                    </OptionNumeric>
                </ConfigSection> 
                <!-- Copper Configuration UI Complete -->
                
                
                <!-- Tin Configuration UI Starting -->
                <ConfigSection>
                    <OptionChoice name='frstTinDist'  displayState='shown' displayGroup='groupForestry'> 
                        <Description> Controls how Tin is generated </Description> 
                        <DisplayName>Forestry Tin</DisplayName>
                        <Choice value='layeredVeins' displayValue='Layered Veins'>
                            <Description>
                                Layered Veins.
                            </Description>
                        </Choice>
                        <Choice value='strategicCloud' displayValue='Clouds'>
                            <Description>
                                Strategic Clouds.
                            </Description>
                        </Choice>
                        <Choice value='vanillaStdGen' displayValue='Vanilla'>
                            <Description>
                                Vanilla-style clusters.
                            </Description>
                        </Choice>
                        <Choice value='none' displayValue='None' description='Tin is not generated in the world.'/>
                    </OptionChoice>
                    <OptionNumeric name='frstTinFreq' default='1'  min='0' max='5' displayState=':= if(?advOptions,"shown","hidden")' displayGroup='groupForestry'>
                        <Description> Frequency multiplier for Forestry Tin distributions </Description>
                        <DisplayName>Forestry Tin Freq.</DisplayName>
                    </OptionNumeric>
                    <OptionNumeric name='frstTinSize' default='1'  min='0' max='5' displayState=':= if(?advOptions,"shown","hidden")' displayGroup='groupForestry'>
                        <Description> Size multiplier for Forestry Tin distributions </Description>
                        <DisplayName>Forestry Tin Size</DisplayName>
                    </OptionNumeric>
                </ConfigSection> 
                <!-- Tin Configuration UI Complete -->
                
            </ConfigSection>
            <!-- Setup Screen Complete -->


            <!-- Setup Overworld -->
            <IfCondition condition=':= ?COGActive'>
                
                <!-- Starting Original Overworld Ore Removal -->
                <Substitute name='frstOverworldOreSubstitute0' block='minecraft:stone'>
                    <Description>
                        Replace vanilla-generated ore clusters.
                    </Description>
                    <Comment>
                        The global option deferredPopulationRange must be large enough to catch all ore clusters (>= 32).
                    </Comment>
                    <Replaces block='Forestry:resources:0' />
                    <Replaces block='Forestry:resources:1' />
                    <Replaces block='Forestry:resources:0' />
                </Substitute>
                <!-- Original Overworld Ore Removal Complete -->
                
                <!-- Adding ores --> 
                
                <!-- Begin Apatite Generation --> 
                
                <!-- Begin SparseVeins distribution of Apatite -->
                <IfCondition condition=':= frstApatiteDist = "sparseVeins"'>
                
                    <Veins name='frstApatite0Veins' block='Forestry:resources' inherits='PresetSparseVeins'>
                        <Description>
                            Large veins filled very lightly with ore.  Because they contain less ore per volume, 
                            these veins are relatively wide and long.  Mining the ore from them is time consuming 
                            compared to solid ore veins.  They are also more difficult to follow, since it is 
                            harder to get an idea of their direction while mining.
                        </Description>
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x60592ab4</WireframeColor>
                        <Setting name='MotherlodeFrequency' avg=':= 1 * 1 * 10.7 * frstApatiteFreq * _default_'/>
                        <Setting name='MotherlodeSize' avg=':= 1 * 1 * frstApatiteSize * _default_' range=':= 1 * 1 * frstApatiteSize * _default_'/>
                        <Setting name='MotherlodeHeight' avg=':= 60' range=':= 10' type='normal' scaleTo='Base' />
                        <Setting name='SegmentRadius' avg=':= 1 * 1 * frstApatiteSize * _default_' range=':= 1 * 1 * frstApatiteSize * _default_'/>
                        <Setting name='OreDensity' avg=':= 1 * 1.75 * _default_' range=':= _default_'/>
                        <Replaces block='minecraft:stone'/>
                    </Veins>
                    
                    <!-- Begin Preferred Biome Distribution (Apatite Sparse Veins) Settings -->
                    <Veins name='frstApatite1Veins' block='Forestry:resources' inherits='PresetSparseVeins'>
                        <Description>
                            Large veins filled very lightly with ore.  Because they contain less ore per volume, 
                            these veins are relatively wide and long.  Mining the ore from them is time consuming 
                            compared to solid ore veins.  They are also more difficult to follow, since it is 
                            harder to get an idea of their direction while mining.
                            2 times as likely in preferred biomes.
                        </Description>
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x60592ab4</WireframeColor>
                        <Setting name='MotherlodeFrequency' avg=':= 2 * 1 * 10.7 * frstApatiteFreq * _default_'/>
                        <Setting name='MotherlodeSize' avg=':= 1 * 1 * frstApatiteSize * _default_' range=':= 1 * 1 * frstApatiteSize * _default_'/>
                        <Setting name='MotherlodeHeight' avg=':= 60' range=':= 10' type='normal' scaleTo='Base' />
                        <Setting name='SegmentRadius' avg=':= 1 * 1 * frstApatiteSize * _default_' range=':= 1 * 1 * frstApatiteSize * _default_'/>
                        <Setting name='OreDensity' avg=':= 1 * 1.75 * _default_' range=':= _default_'/>
                        <Replaces block='minecraft:stone'/>
                        <BiomeType name='Forest'/>
                    </Veins>
                    <!-- End Preferred Biome Distribution (Apatite Sparse Veins) Settings -->
                
                </IfCondition>
                <!-- End SparseVeins distribution of Apatite -->
                
                
                <!-- Begin  StrategicCloud distribution of Apatite -->
                <IfCondition condition=':= frstApatiteDist = "strategicCloud"'>
                
                    <Cloud name='frstApatite0Cloud' block='Forestry:resources' inherits='PresetStrategicCloud'>
                        <Description>
                            Large irregular clouds filled lightly with ore.  These are huge, spanning several 
                            adjacent chunks, and consequently rather rare.  They contain a sizeable amount of 
                            ore, but it takes some time and effort to mine due to low density.
                            The intent for strategic clouds is that the player will need to actively search for
                            one and then set up a semi-permanent mining base and spend some time actually mining
                            the ore.
                        </Description>
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x60592ab4</WireframeColor>
                        <Setting name='DistributionFrequency' avg=':= 1 * 1 * 1 * frstApatiteFreq *_default_'/>
                        <Setting name='CloudRadius' avg=':= 1 * 1 * frstApatiteSize * _default_' range=':= 1 * 1 * frstApatiteSize * _default_'/>
                        <Setting name='CloudThickness' avg=':= 1 * 1 * frstApatiteSize * _default_' range=':= 1 * 1 * frstApatiteSize * _default_'/>
                        <Setting name='CloudHeight' avg='60' range='10' type='normal' scaleTo='Base'/>
                        <Setting name='OreDensity' avg=':= 1 * 0.75 * _default_' range=':= _default_'/>
                        <Setting name='CloudThickness' avg=':= 1 * 1 * 1 * frstApatiteSize * _default_' range=':= 1 * 1 * 1 * frstApatiteSize  * _default_'/>
                        <Replaces block='minecraft:stone'/>
                        
                        <!-- Begin Apatite Strategic Cloud Hint Veins -->
                        <Veins name='frstApatite0HintVeins' block='Forestry:resources' inherits='PresetHintVeins'>
                            <Description>
                                Small, fairly rare motherlodes with 2-4 horizontal veins each.
                            </Description>
                            <DrawWireframe>:=drawWireframes</DrawWireframe>
                            <WireframeColor>0x60592ab4</WireframeColor>
                            <Setting name='MotherlodeFrequency' avg=':= 1 * 1.2 * 1 * 1 * frstApatiteFreq * _default_' range=':= _default_'/> 
                            <Setting name='MotherlodeHeight' avg=':= 60' range=':= 10' type='normal' scaleTo='Base' /> 
                            <Setting name='MotherlodeRangeLimit' avg=':= 1 * 1 * frstApatiteSize * _default_' range=':= 1 * 1 * frstApatiteSize * _default_'/>
                            <Setting name='OreDensity' avg=':= 1 * 0.75 * _default_' range=':= _default_'/>
                        </Veins>
                        <!-- End Apatite Strategic Cloud Hint Veins -->

                    </Cloud>
                    
                    <!-- Begin Preferred Biome Distribution (Apatite Strategic Cloud) Settings -->
                    <Cloud name='frstApatite1Cloud' block='Forestry:resources' inherits='PresetStrategicCloud'>
                        <Description>
                            Large irregular clouds filled lightly with ore.  These are huge, spanning several 
                            adjacent chunks, and consequently rather rare.  They contain a sizeable amount of 
                            ore, but it takes some time and effort to mine due to low density.
                            The intent for strategic clouds is that the player will need to actively search for
                            one and then set up a semi-permanent mining base and spend some time actually mining
                            the ore.
                            2 times as likely in preferred biomes.
                        </Description>
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x60592ab4</WireframeColor>
                        <Setting name='DistributionFrequency' avg=':= 2 * 1 * 1 * frstApatiteFreq *_default_'/>
                        <Setting name='CloudRadius' avg=':= 1 * 1 * frstApatiteSize * _default_' range=':= 1 * 1 * frstApatiteSize * _default_'/>
                        <Setting name='CloudThickness' avg=':= 1 * 1 * frstApatiteSize * _default_' range=':= 1 * 1 * frstApatiteSize * _default_'/>
                        <Setting name='CloudHeight' avg='60' range='10' type='normal' scaleTo='Base'/>
                        <Setting name='OreDensity' avg=':= 1 * 0.75 * _default_' range=':= _default_'/>
                        <Setting name='CloudThickness' avg=':= 1 * 1 * 1 * frstApatiteSize * _default_' range=':= 1 * 1 * 1 * frstApatiteSize  * _default_'/>
                        <Replaces block='minecraft:stone'/>
                        <BiomeType name='Forest'/>
                        
                        <!-- Begin Apatite Strategic Cloud Hint Veins -->
                        <Veins name='frstApatite1HintVeins' block='Forestry:resources' inherits='PresetHintVeins'>
                            <Description>
                                Small, fairly rare motherlodes with 2-4 horizontal veins each.
                                2 times as likely in preferred biomes.
                            </Description>
                            <DrawWireframe>:=drawWireframes</DrawWireframe>
                            <WireframeColor>0x60592ab4</WireframeColor>
                            <Setting name='MotherlodeFrequency' avg=':= 2 * 1.2 * 1 * 1 * frstApatiteFreq * _default_' range=':= _default_'/> 
                            <Setting name='MotherlodeHeight' avg=':= 60' range=':= 10' type='normal' scaleTo='Base' /> 
                            <Setting name='MotherlodeRangeLimit' avg=':= 1 * 1 * frstApatiteSize * _default_' range=':= 1 * 1 * frstApatiteSize * _default_'/>
                            <Setting name='OreDensity' avg=':= 1 * 0.75 * _default_' range=':= _default_'/>
                            <BiomeType name='Forest'/>
                        </Veins>
                        <!-- End Apatite Strategic Cloud Hint Veins -->

                    </Cloud>
                    <!-- End Preferred Biome Distribution (Apatite Strategic Cloud) Settings -->
                
                </IfCondition>
                <!-- End  StrategicCloud distribution of Apatite -->
                
                
                <!-- Begin  Vanilla distribution of Apatite -->
                <IfCondition condition=':= frstApatiteDist = "vanillaStdGen"'>
                
                    <StandardGen name='frstApatite0Standard' block='Forestry:resources' inherits='PresetStandardGen'>
                        <Description>
                            This mimics vanilla ore generation.
                        </Description>
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x60592ab4</WireframeColor>
                        <Setting name='Size' avg=':= 1 * 2 * frstApatiteSize * _default_'/>
                        <Setting name='Frequency' avg=':= 1 * 1 * 1 * frstApatiteFreq * _default_'/>
                        <Setting name='Height' avg=':= 60' range=':= 10'/> 
                        <Replaces block='minecraft:stone'/>
                    </StandardGen>
                    
                    <!-- Begin Preferred Biome Distribution (Apatite Vanilla) Settings -->
                    <StandardGen name='frstApatite1Standard' block='Forestry:resources' inherits='PresetStandardGen'>
                        <Description>
                            This mimics vanilla ore generation.
                            2 times as likely in preferred biomes.
                        </Description>
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x60592ab4</WireframeColor>
                        <Setting name='Size' avg=':= 1 * 2 * frstApatiteSize * _default_'/>
                        <Setting name='Frequency' avg=':= 2 * 1 * 1 * frstApatiteFreq * _default_'/>
                        <Setting name='Height' avg=':= 60' range=':= 10'/> 
                        <Replaces block='minecraft:stone'/>
                        <BiomeType name='Forest'/>
                    </StandardGen>
                    <!-- End Preferred Biome Distribution (Apatite Vanilla) Settings -->
                
                </IfCondition>
                <!-- End  Vanilla distribution of Apatite -->
                
                <!-- End Apatite Generation --> 

                
                <!-- Begin Copper Generation --> 
                
                <!-- Begin LayeredVeins distribution of Copper -->
                <IfCondition condition=':= frstCopperDist = "layeredVeins"'>
                
                    <Veins name='frstCopper0Veins' block='Forestry:resources:1' inherits='PresetLayeredVeins'>
                        <Description>
                            Small, fairly rare motherlodes with 2-4 horizontal veins each.
                        </Description>
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x60592ab4</WireframeColor>
                        <Setting name='MotherlodeSize' avg=':= 1 * 1 * frstCopperSize * _default_' range=':= 1 * 1 * frstCopperSize * _default_'/>
                        <Setting name='MotherlodeFrequency' avg=':= 1 * 1 * 0.9 * frstCopperFreq * _default_'/>
                        <Setting name='MotherlodeHeight' avg=':= 43' range=':= 10.5' type='normal' scaleTo='Base'/> 
                        <Setting name='SegmentRadius' avg=':= 1 * 1 * frstCopperSize * _default_' range=':= 1 * 1 * frstCopperSize * _default_'/>
                        <Setting name='OreDensity' avg=':= 1 * 1 * _default_' range=':= _default_'/>
                        <Replaces block='minecraft:stone'/>
                    </Veins>
                    
                    <!-- Begin Preferred Biome Distribution (Copper Layered Veins) Settings -->
                    <Veins name='frstCopper1Veins' block='Forestry:resources:1' inherits='PresetLayeredVeins'>
                        <Description>
                            Small, fairly rare motherlodes with 2-4 horizontal veins each.
                            2 times as likely in preferred biomes.
                        </Description>
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x60592ab4</WireframeColor>
                        <Setting name='MotherlodeSize' avg=':= 1 * 1 * frstCopperSize * _default_' range=':= 1 * 1 * frstCopperSize * _default_'/>
                        <Setting name='MotherlodeFrequency' avg=':= 2 * 1 * 0.9 * frstCopperFreq * _default_'/>
                        <Setting name='MotherlodeHeight' avg=':= 43' range=':= 10.5' type='normal' scaleTo='Base'/> 
                        <Setting name='SegmentRadius' avg=':= 1 * 1 * frstCopperSize * _default_' range=':= 1 * 1 * frstCopperSize * _default_'/>
                        <Setting name='OreDensity' avg=':= 1 * 1 * _default_' range=':= _default_'/>
                        <Replaces block='minecraft:stone'/>
                        <BiomeType name='Cold'/>
                    </Veins>
                    <!-- End Preferred Biome Distribution (Copper Layered Veins) Settings -->
                
                </IfCondition>
                <!-- End LayeredVeins distribution of Copper -->
                
                
                <!-- Begin  StrategicCloud distribution of Copper -->
                <IfCondition condition=':= frstCopperDist = "strategicCloud"'>
                
                    <Cloud name='frstCopper0Cloud' block='Forestry:resources:1' inherits='PresetStrategicCloud'>
                        <Description>
                            Large irregular clouds filled lightly with ore.  These are huge, spanning several 
                            adjacent chunks, and consequently rather rare.  They contain a sizeable amount of 
                            ore, but it takes some time and effort to mine due to low density.
                            The intent for strategic clouds is that the player will need to actively search for
                            one and then set up a semi-permanent mining base and spend some time actually mining
                            the ore.
                        </Description>
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x60592ab4</WireframeColor>
                        <Setting name='DistributionFrequency' avg=':= 1 * 1 * 2.5 * frstCopperFreq *_default_'/>
                        <Setting name='CloudRadius' avg=':= 1 * 1 * frstCopperSize * _default_' range=':= 1 * 1 * frstCopperSize * _default_'/>
                        <Setting name='CloudThickness' avg=':= 1 * 1 * frstCopperSize * _default_' range=':= 1 * 1 * frstCopperSize * _default_'/>
                        <Setting name='CloudHeight' avg='43' range='10.5' type='normal' scaleTo='Base'/>
                        <Setting name='OreDensity' avg=':= 1 * 1 * _default_' range=':= _default_'/>
                        <Setting name='CloudThickness' avg=':= 1 * 1 * 1 * frstCopperSize * _default_' range=':= 1 * 1 * 1 * frstCopperSize  * _default_'/>
                        <Replaces block='minecraft:stone'/>
                        
                        <!-- Begin Copper Strategic Cloud Hint Veins -->
                        <Veins name='frstCopper0HintVeins' block='Forestry:resources:1' inherits='PresetHintVeins'>
                            <Description>
                                Small, fairly rare motherlodes with 2-4 horizontal veins each.
                            </Description>
                            <DrawWireframe>:=drawWireframes</DrawWireframe>
                            <WireframeColor>0x60592ab4</WireframeColor>
                            <Setting name='MotherlodeFrequency' avg=':= 1 * 1.2 * 1 * 2.5 * frstCopperFreq * _default_' range=':= _default_'/> 
                            <Setting name='MotherlodeHeight' avg=':= 43' range=':= 10.5' type='normal' scaleTo='Base' /> 
                            <Setting name='MotherlodeRangeLimit' avg=':= 1 * 1 * frstCopperSize * _default_' range=':= 1 * 1 * frstCopperSize * _default_'/>
                            <Setting name='OreDensity' avg=':= 1 * 1 * _default_' range=':= _default_'/>
                        </Veins>
                        <!-- End Copper Strategic Cloud Hint Veins -->

                    </Cloud>
                    
                    <!-- Begin Preferred Biome Distribution (Copper Strategic Cloud) Settings -->
                    <Cloud name='frstCopper1Cloud' block='Forestry:resources:1' inherits='PresetStrategicCloud'>
                        <Description>
                            Large irregular clouds filled lightly with ore.  These are huge, spanning several 
                            adjacent chunks, and consequently rather rare.  They contain a sizeable amount of 
                            ore, but it takes some time and effort to mine due to low density.
                            The intent for strategic clouds is that the player will need to actively search for
                            one and then set up a semi-permanent mining base and spend some time actually mining
                            the ore.
                            2 times as likely in preferred biomes.
                        </Description>
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x60592ab4</WireframeColor>
                        <Setting name='DistributionFrequency' avg=':= 2 * 1 * 2.5 * frstCopperFreq *_default_'/>
                        <Setting name='CloudRadius' avg=':= 1 * 1 * frstCopperSize * _default_' range=':= 1 * 1 * frstCopperSize * _default_'/>
                        <Setting name='CloudThickness' avg=':= 1 * 1 * frstCopperSize * _default_' range=':= 1 * 1 * frstCopperSize * _default_'/>
                        <Setting name='CloudHeight' avg='43' range='10.5' type='normal' scaleTo='Base'/>
                        <Setting name='OreDensity' avg=':= 1 * 1 * _default_' range=':= _default_'/>
                        <Setting name='CloudThickness' avg=':= 1 * 1 * 1 * frstCopperSize * _default_' range=':= 1 * 1 * 1 * frstCopperSize  * _default_'/>
                        <Replaces block='minecraft:stone'/>
                        <BiomeType name='Cold'/>
                        
                        <!-- Begin Copper Strategic Cloud Hint Veins -->
                        <Veins name='frstCopper1HintVeins' block='Forestry:resources:1' inherits='PresetHintVeins'>
                            <Description>
                                Small, fairly rare motherlodes with 2-4 horizontal veins each.
                                2 times as likely in preferred biomes.
                            </Description>
                            <DrawWireframe>:=drawWireframes</DrawWireframe>
                            <WireframeColor>0x60592ab4</WireframeColor>
                            <Setting name='MotherlodeFrequency' avg=':= 2 * 1.2 * 1 * 2.5 * frstCopperFreq * _default_' range=':= _default_'/> 
                            <Setting name='MotherlodeHeight' avg=':= 43' range=':= 10.5' type='normal' scaleTo='Base' /> 
                            <Setting name='MotherlodeRangeLimit' avg=':= 1 * 1 * frstCopperSize * _default_' range=':= 1 * 1 * frstCopperSize * _default_'/>
                            <Setting name='OreDensity' avg=':= 1 * 1 * _default_' range=':= _default_'/>
                            <BiomeType name='Cold'/>
                        </Veins>
                        <!-- End Copper Strategic Cloud Hint Veins -->

                    </Cloud>
                    <!-- End Preferred Biome Distribution (Copper Strategic Cloud) Settings -->
                
                </IfCondition>
                <!-- End  StrategicCloud distribution of Copper -->
                
                
                <!-- Begin  Vanilla distribution of Copper -->
                <IfCondition condition=':= frstCopperDist = "vanillaStdGen"'>
                
                    <StandardGen name='frstCopper0Standard' block='Forestry:resources:1' inherits='PresetStandardGen'>
                        <Description>
                            This mimics vanilla ore generation.
                        </Description>
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x60592ab4</WireframeColor>
                        <Setting name='Size' avg=':= 1 * 1 * frstCopperSize * _default_'/>
                        <Setting name='Frequency' avg=':= 1 * 1 * 1 * frstCopperFreq * _default_'/>
                        <Setting name='Height' avg=':= 43' range=':= 10.5'/> 
                        <Replaces block='minecraft:stone'/>
                    </StandardGen>
                    
                    <!-- Begin Preferred Biome Distribution (Copper Vanilla) Settings -->
                    <StandardGen name='frstCopper1Standard' block='Forestry:resources:1' inherits='PresetStandardGen'>
                        <Description>
                            This mimics vanilla ore generation.
                            2 times as likely in preferred biomes.
                        </Description>
                        <DrawWireframe>:=drawWireframes</DrawWireframe>
                        <WireframeColor>0x60592ab4</WireframeColor>
                        <Setting name='Size' avg=':= 1 * 1 * frstCopperSize * _default_'/>
                        <Setting name='Frequency' avg=':= 2 * 1 * 1 * frstCopperFreq * _default_'/>
                        <Setting name='Height' avg=':= 43' range=':= 10.5'/> 
                        <Replaces block='minecraft:stone'/>
                        <BiomeType name='Cold'/>
                    </StandardGen>
                    <!-- End Preferred Biome Distribution (Copper Vanilla) Settings -->
                
                </IfCondition>
                <!-- End  Vanilla distribution of Copper -->
                
                <!-- End Copper Generation --> 

                
                <!-- Begin Tin Generation --> 
                
                <!-- Begin LayeredVeins distribution of Tin -->
                <IfCondition condition=':= frstTinDist = "layeredVeins"'>
                
                    <Veins name='frstTin0Veins' block='Forestry:resources' inherits='PresetLayeredVeins'>
                        <Description>
                            Small, fairly rare motherlodes with 2-4 horizontal veins each.
                        </Description>
