                        <WireframeColor>0x60592ab4</WireframeColor>
                        <Setting name='DistributionFrequency' avg=':= 1 * 1 * 2.5 * frstCopperFreq *_default_'/>
                        <Setting name='CloudRadius' avg=':= 1 * 1 * frstCopperSize * _default_' range=':= 1 * 1 * frstCopperSize * _default_'/>
                        <Setting name='CloudThickness' avg=':= 1 * 1 * frstCopperSize * _default_' range=':= 1 * 1 * frstCopperSize * _default_'/>
                        <Setting name='CloudHeight' avg='43' range='10.5' type='normal' scaleTo='Base'/>
                        <Setting name='OreDensity' avg=':= 1 * 1 * _default_' range=':= _default_'/>
                        <Setting name='CloudThickness' avg=':= 1 * 1 * 1 * frstCopperSize * _default_' range=':= 1 * 1 * 1 * frstCopperSize  * _default_'/>
                        <Replaces block='minecraft:stone'/>
